触发
_createUVMatrixData() {
    var e = this[DI];
    return e.uvData || (e.uvData = new fr({
        data: {
            map: lL,
            envMap: lL,
            alphaMap: lL,
            emissiveMap: lL,
            normalMap: lL,
            colorMapping: lL,
            aoMap: lL
        },
        onChange: t=>{
            var r = e.curImageSlotType
                , n = t.data[cL(r)];
            n && this._updateImageUVMatrix(r, n)
        }
    })),
    e.uvData
}



